{
  "name": "vue-text-ellipsis",
  "version": "1.0.0",
  "author": "112KA",
  "license": "MIT",
  "main": "dist/vue-text-ellipsis.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/112KA/vue-text-ellipsis"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:example": "vue-cli-service build --dest example",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist//vue-text-ellipsis.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist//vue-text-ellipsis.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist//vue-text-ellipsis.min.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "node-sass": "^4.9.0",
    "rollup": "^1.12.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "keywords": [
    "vue",
    "text",
    "ellipsis"
  ]
}